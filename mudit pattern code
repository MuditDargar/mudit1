#include <iostream>
using namespace std ;
int main (){
    int n;
    cin >> n ;
    int row = 1;
    while ( row <= n){
        int col =1 ;
        while( col <=  n -row +1){
            cout<< col ;
            col = col +1 ;        
 }
    int pet =1 ;
 while (  pet <= row -1){
cout <<  "*";
pet = pet +1 ;
 }
 int cee = 1 ;
 while( cee <= row -1 ){
    cout<< "*" ;
    cee = cee +1 ;
 }

 int start = col -1   ;
 while ( start){
    cout << start ;
    start = start  - 1 ;
 
 }
 row = row +1 ;
 cout << endl ;
    }
}
